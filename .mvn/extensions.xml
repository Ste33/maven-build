<?xml version="1.0" encoding="UTF-8"?>
<!--
see:
[Add ability to easily retrieve core extensions from alternative pluginRepository](https://issues.apache.org/jira/browse/MNG-6327)
[Maven core extension resolution ignores repositories from activeByDefault profiles in settings.xml](https://issues.apache.org/jira/browse/MNG-5984)
-->
<extensions>
    <!-- mvn install -Dprofile="profile name" -DprofileFormat=JSON,HTML -->
    <extension>
        <groupId>fr.jcgay.maven</groupId>
        <artifactId>maven-profiler</artifactId>
        <version>2.6</version>
    </extension>
    <extension>
        <groupId>top.infra</groupId>
        <artifactId>maven-build-extension</artifactId>
        <version>0.0.1-SNAPSHOT</version>
    </extension>

    <!-- see: https://github.com/random-maven/profile-activator-extension/issues/3
    <extension>
        <groupId>com.carrotgarden.maven</groupId>
        <artifactId>arkon-maven-extension</artifactId>
        <version>1.3.20190513180922</version>
    </extension>
    -->
    <!-- profile-activator-extension conflicts with maven-profiledep-extension
    <extension>
        <groupId>com.carrotgarden.maven</groupId>
        <artifactId>profile-activator-extension</artifactId>
        <version>1.2.20180119141943</version>
    </extension>
    -->
    <!-- maven-profiledep-extension conflicts with profile-activator-extension
    <extension>
        <groupId>com.github.sviperll</groupId>
        <artifactId>maven-profiledep-extension</artifactId>
        <version>0.23</version>
    </extension>
    -->
    <!--
    <extension>
        <groupId>io.takari.maven</groupId>
        <artifactId>takari-smart-builder</artifactId>
        <version>0.6.1</version>
    </extension>
    -->
</extensions>
